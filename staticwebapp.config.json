{
  "trailingSlash": "auto",
  "routes": [
    {
      "route": "/api/*",
      "methods": ["PUT", "POST", "PATCH", "DELETE","GET"],
      "headers": {
              "Access-Control-Allow-Origin": "[*]",
              "Access-Control-Allow-Methods": "GET, POST, OPTIONS"
      }
    },
    {
      "route": "/data-api/rest/*",
      "methods": ["PUT", "POST", "PATCH", "DELETE","GET"],
      "headers": {
              "Access-Control-Allow-Origin": "[*]",
              "Access-Control-Allow-Methods": "GET, POST, OPTIONS"
      }
    }
  ],
  "globalHeaders": {
    "Access-Control-Allow-Origin": "[*]",
    "Access-Control-Allow-Methods": "GET, POST, OPTIONS"
  }
}

{
    "routes": [
        {
            "route": "/",
            "methods": ["PUT", "POST", "PATCH", "DELETE","GET"]
        },
        {
      "route": "/data-api/rest/*",
      "methods": ["PUT", "POST", "PATCH", "DELETE","GET"]
    }
    ],
    "navigationFallback": {
        "rewrite": "/index.html"
    },
    "globalHeaders": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "POST, GET, OPTIONS"
    }
}
